<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.shago_000.puzzle" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="2048"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="2048" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".custom_dialogue"
            android:label="custome"
            android:parentActivityName=".MainActivity"
            android:theme="@android:style/Theme.Dialog" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.shago_000.puzzle.MainActivity" />
        </activity>
        <activity
            android:name=".win_dialogue"
            android:label="@string/title_activity_win_dialogue"
            android:theme="@android:style/Theme.Dialog"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.shago_000.puzzle.MainActivity" />
        </activity>
    </application>

</manifest>
